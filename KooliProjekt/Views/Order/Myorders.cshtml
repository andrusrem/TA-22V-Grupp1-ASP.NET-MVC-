@model List<KooliProjekt.Data.Myorders>

<h1>Your Orders</h1>

@if(Model.Count == 0)
    {
        <h4>You do not have Orders yet.</h4>
    }

@foreach (var order in Model)
{
    <div>
        <h3>Order ID: @order.Id</h3>
        <p>Customer ID: @order.CustomerId</p>
        <p>Estimated Price: @order.EstimatedPrice</p>
        <p>Product ID: @order.ProductName</p>
    </div>
}